{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "essential": false,
            "hostname": "client",
            "image": "abechraoui/multi-client",
            "name": "client"
        },
        {
            "essential": false,
            "hostname": "api",
            "image": "abechraoui/multi-server",
            "name": "server"
        },
        {
            "essential": false,
            "hostname": "worker",
            "image": "abechraoui/multi-worker",
            "name": "worker"
        },
        {
            "essential": true,
            "hostname": "nginx",
            "image": "abechraoui/multi-nginx",
            "links": [
                "client",
                "server"
            ],
            "name": "nginx",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}